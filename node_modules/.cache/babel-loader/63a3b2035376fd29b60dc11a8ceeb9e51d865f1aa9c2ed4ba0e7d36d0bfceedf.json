{"ast":null,"code":"import Header from './components/Header';\nimport Tasks from './components/Tasks';\nimport AddTask from './components/AddTask';\nexport default {\n  name: 'App',\n  components: {\n    Header,\n    Tasks,\n    AddTask\n  },\n  data() {\n    return {\n      tasks: []\n    };\n  },\n  methods: {\n    addTask(task) {\n      this.tasks = [...this.tasks, task];\n    },\n    deleteTask(id) {\n      console.log('i am in delete ');\n      if (confirm('Are You Sure?')) {\n        this.tasks = this.tasks.filter(task => task.id !== id);\n      }\n    },\n    toggleReminder(id) {\n      console.log('i am in toggle');\n      this.tasks = this.tasks.map(task => task.id === id ? {\n        ...task,\n        reminder: !task.reminder\n      } : task);\n    }\n  },\n  created() {\n    this.tasks = [{\n      id: 1,\n      text: 'Doctor Appointment',\n      day: 'March 1st at 2:30pm',\n      reminder: true\n    }, {\n      id: 2,\n      text: 'Meeting at school',\n      day: 'March 2nd at 2:30pm',\n      reminder: true\n    }, {\n      id: 3,\n      text: 'Food Shopping',\n      day: 'March 3rd at 2:30pm',\n      reminder: false\n    }];\n  }\n};","map":{"version":3,"mappings":"AAUA,OAAOA,MAAK,MAAO,qBAAoB;AACvC,OAAOC,KAAI,MAAO,oBAAmB;AACrC,OAAOC,OAAM,MAAO,sBAAqB;AAEzC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,MAAM;IACNC,KAAK;IACLC;EAEF,CAAC;EACDG,IAAI,GAAE;IACJ,OAAM;MACJC,KAAK,EAAC;IACR;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,OAAO,CAACC,IAAI,EAAC;MACX,IAAI,CAACH,KAAK,GAAC,CAAC,GAAG,IAAI,CAACA,KAAK,EAAEG,IAAI;IACjC,CAAC;IAEDC,UAAU,CAACC,EAAE,EAAC;MACZC,OAAO,CAACC,GAAG,CAAC,iBAAiB;MAC7B,IAAGC,OAAO,CAAC,eAAe,CAAC,EAAC;QAC9B,IAAI,CAACR,KAAK,GAAC,IAAI,CAACA,KAAK,CAACS,MAAM,CAAEN,IAAI,IAAGA,IAAI,CAACE,EAAC,KAAKA,EAAE;MACpD;IACF,CAAC;IACCK,cAAc,CAACL,EAAE,EAAC;MAEhBC,OAAO,CAACC,GAAG,CAAC,gBAAgB;MAC5B,IAAI,CAACP,KAAK,GAAC,IAAI,CAACA,KAAK,CAACW,GAAG,CAAER,IAAI,IAAGA,IAAI,CAACE,EAAE,KAAGA,EAAE,GAAC;QAAC,GAAGF,IAAI;QAACS,QAAQ,EAAE,CAACT,IAAI,CAACS;MAAQ,CAAC,GAAET,IAAI;IACzF;EACA,